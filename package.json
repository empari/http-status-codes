{
  "name": "@empari/http-status-codes",
  "description": "Constants enumerating the HTTP status codes. Based on the Java Apache HttpStatus API",
  "version": "1.0.3",
  "main": "index.js",
  "repository": "git@github.com:empari/http-status-codes.git",
  "author": "Team Empari <suporte@empari.com.br>",
  "license": "MIT",
  "scripts": {
    "commit": "git-cz",
    "test": "jest --passWithNoTests --colors --noStackTrace --runInBand",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test -- --coverage",
    "build": "tsc --declaration"
  },
  "keywords": [
    "node",
    "javascript",
    "typescript",
    "http",
    "apache",
    "HttpStatus",
    "httpclient",
    "status",
    "codes"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^24.0.25",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-with-typescript": "^11.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.3.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.0.1",
    "jest": "^24.9.0",
    "jsmin": "^1.0.1",
    "lint-staged": "^9.5.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
